# interfaces(5) file used by ifup(8) and ifdown(8)
# Include files from /etc/network/interfaces.d:
source-directory /etc/network/interfaces.d

# The loopback network interface
auto lo
iface lo inet loopback

# The primary network interface
#VAGRANT-BEGIN
# The contents below are automatically generated by Vagrant. Do not modify.
auto enp1s0
iface enp1s0 inet dhcp

auto enp8s0
iface enp8s0 inet static
      address 192.168.200.10
      netmask 255.255.255.0

auto enp7s0
iface enp7s0 inet static
      address 10.0.0.10
      netmask 255.255.255.0
#VAGRANT-END
#up iptables -A -t nat POSTROUTING -s 10.0.0.0/24 -o eth1 -j MASQUERADE
#up iptables -t nat -A PREROUTING -p tcp --dport 80 -i eth1 -j DNAT --to 10.0.0.11
##                    PARA EL DNAT                 interfaz ruter público hacia 10.0.0.11
up iptables -t nat -A PREROUTING -p tcp --dport 22 -i enp1s0 -j DNAT --to 10.0.0.11
up iptables -t nat -A PREROUTING -p tcp --dport 80 -i enp1s0 -j DNAT --to 10.0.0.11
##                    PARA EL SNAT                hacia interfaz pública router con cualquier ip
up iptables -t nat -A POSTROUTING -s 10.0.0.0/24 -o enp1s0 -j MASQUERADE
